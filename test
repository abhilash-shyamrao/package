CREATE TABLE dqc_schema.execution_group (
    execution_group_id SERIAL PRIMARY KEY,
    execution_group_name VARCHAR(255),
    execution_group_description TEXT,
    execution_group_schedule VARCHAR(255),
    execution_group_status VARCHAR(50),
    last_execution_time TIMESTAMP,
    next_execution_time TIMESTAMP,
    execution_group_region VARCHAR(50),
    created_date TIMESTAMP,
    updated_date TIMESTAMP
);
