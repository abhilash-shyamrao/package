CREATE TABLE data_quality_config_dataset_filter (
    dq_config_dataset_filter_id SERIAL PRIMARY KEY,
    dq_config_id INT,
    dataset_id INT,
    filter_string TEXT,
    created_date TIMESTAMP,
    created_by VARCHAR(50),
    updated_by VARCHAR(50),
    updated_date TIMESTAMP,
    
    -- Foreign key constraints
    CONSTRAINT fk_dq_config
        FOREIGN KEY (dq_config_id)
        REFERENCES dqc_schema.data_quality_config(dq_config_id)
        ON DELETE CASCADE,
    
    CONSTRAINT fk_dataset
        FOREIGN KEY (dataset_id)
        REFERENCES dataset(dataset_id)
        ON DELETE CASCADE
);
